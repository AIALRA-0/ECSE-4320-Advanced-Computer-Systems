[global]
ioengine=io_uring
direct=1
time_based=1
ramp_time=5
runtime=30
group_reporting=1
refill_buffers=1
buffer_compress_percentage=0
numjobs=1
numjobs=1
iodepth=1
thread=1
percentile_list=50:95:99:99.9
filename=/dev/nvme1n1p1


[randread_qd1]
rw=randread
bs=4k
offset_align=4k

[randwrite_qd1]
rw=randwrite
bs=4k
offset_align=4k

[read128k_qd1]
rw=read
bs=128k

[write128k_qd1]
rw=write
bs=128k
